<? php

print "média de cigarros fumados por dia:";
    $cigarros_fumados = fgets (STDIN);
print "dias como fumante:";
    $dias_fumados = fgets (STDIN);

    $minutos_dia = $cigarros_fumados * 10;
    $dias_perdidos = $dias_fumados * $miutos_dia;
    $dias_perdidos = round ($dias_perdidos/1440);

print "$dias_perdidos";
