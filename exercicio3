<? php

print "salário atual:";
    $salario = fgets (STDIN);
 print "porcentagem do aumento:";
    $porcentagem_aumento = fgets (STDIN);

    $aumento_salario = $salario * ($porcentagem_aumento / 100);
    $salario_novo = $salario + $aumento_salario;

print "valor ao salário: $aumento_salario \ n";
print "salário novo: $salario_novo";
